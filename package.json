    "babel-polyfill": "^6.9.1",
    "babel-preset-react-native": "^1.9.0",
    "enzyme": "^2.4.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.18.0",
    "expect-jsx": "^2.3.0",
    "flow-bin": "^0.28.0",
    "jest-cli": "^13.2.3",
    "promise-sync-es6": "0.0.4",
